@article{seabraLifeCycleAssessment2011,
  title = {Life Cycle Assessment of {{Brazilian}} Sugarcane Products: {{GHG}} Emissions and Energy Use},
  shorttitle = {Life Cycle Assessment of {{Brazilian}} Sugarcane Products},
  author = {Seabra, Joaquim E. A. and Macedo, Isaias C. and Chum, Helena L. and Faroni, Carlos E. and Sarto, Celso A.},
  year = {2011},
  month = sep,
  journal = {Biofuels, Bioproducts and Biorefining},
