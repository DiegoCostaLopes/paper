  author = {Miranda, Denis da Silva and Martins, Luise Prado and Teles, Beatriz Arioli de S{\'a} and Cunha, Isadora L. C. and Menezes, Nat{\'a}lia de Almeida and Sakamoto, Hugo and Kulay, Luiz},
  year = {2023},
