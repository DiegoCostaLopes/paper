@article{kushwahModellingApproachesBiomass2022,
  title = {Modelling Approaches for Biomass Gasifiers: {{A}} Comprehensive Overview},
  shorttitle = {Modelling Approaches for Biomass Gasifiers},
  author = {Kushwah, A. and Reina, T. R. and Short, M.},
  year = {2022},
