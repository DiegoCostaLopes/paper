  author = {Chen, Yuhong and Lyu, Yanfeng and Yang, Xiangdong and Zhang, Xiaohong and Pan, Hengyu and Wu, Jun and Lei, Yongjia and Zhang, Yanzong and Wang, Guiyin and Xu, Min and Luo, Hongbin},
