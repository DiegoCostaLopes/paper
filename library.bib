@article{houKineticsMethaneSteam2001,
  title = {The Kinetics of Methane Steam Reforming over a {{Ni}}/{$\alpha$}-{{Al2O}} Catalyst},
  author = {Hou, Kaihu and Hughes, Ronald},
  year = {2001},
  month = mar,
  journal = {Chemical Engineering Journal},
  series = {{{FRONTIERS IN CHEMICAL REACTION ENGINEERING}}},
  volume = {82},
  number = {1},
  pages = {311--328},
  issn = {1385-8947},
  doi = {10.1016/S1385-8947(00)00367-3},
  urldate = {2022-10-21},
  abstract = {Experiments have been carried out to study the kinetics of the methane steam reforming, accompanied by the reverse water gas shift reaction over a commercial Ni/{$\alpha$}-Al2O3 catalyst in an integral reactor under conditions of no diffusion limitation. The experiments demonstrated that both CO and CO2 are formed as primary products, and the rate of methane disappearance is proportional to the partial pressure of methane at low product concentrations. The effect of total pressure on initial reaction rates indicated that the rate controlling steps of steam reforming are surface reactions between adsorbed species. Six possible reaction mechanisms were considered in detail, and intrinsic rate equations were derived by using the Langmuir--Hinshelwood--Hougen--Watson (LH--HW) approach and Freundlich's adsorption concept. Applying the method of parameter estimation and model discrimination, a satisfactory model of intrinsic kinetics for methane steam reforming over the catalyst used was determined. Good agreement was obtained between the experimental data and results predicted from the kinetic model.},
  langid = {english},
  keywords = {Intrinsic kinetics,Methane steam reforming,Nickel catalyst},
  file = {/home/diego/Drive/Library/PhD/SMR/Hou_Hughes_2001_The kinetics of methane steam reforming over a Ni-α-Al2O catalyst.pdf}
}

@article{huangInvestigationBiomassPine2019,
  title = {Investigation of Biomass (Pine Wood) Gasification: {{Experiments}} and {{Aspen Plus}} Simulation},
  shorttitle = {Investigation of Biomass (Pine Wood) Gasification},
  author = {Huang, Fenxia and Jin, Shiping},
  year = {2019},
  month = apr,
  journal = {Energy Science \& Engineering},
  volume = {7},
  doi = {10.1002/ese3.338},
  abstract = {Biomass gasification is currently a hot research topic. To achieve a high hydrogen content in the product gas, the gasification feedstock used in this study is air-dried pine woodchips. Experiments are performed in a downdraft gasifier by varying the operation parameters of the particle size (60 mesh, 80 mesh, 100 mesh), temperature point (700, 750, 800, 850, 900{$^\circ$}C), and steam-to-biomass mass (S/B) ratio (0, 0.7, 1.4, 2.1, 2.8). The main effects of particle size, temperature, and S/B ratio on the composition of the product gas are analyzed to predict the optimal operation parameters of biomass feedstock. For pine woodchip gasification, the optimal particle size is 80 mesh or 0.17 mm, the preferred temperature is 850{$^\circ$}C, and the optimal S/B ratio is 1.4. Although there is an error between the experiment and the simulation, the difference is not significant. The Aspen Plus model can provide guidance for the gasification of pine woodchips and can be extended to the gasification of other kinds of biomass. The main effects of particle size, temperature, and S/B ratio on the composition of the product gas are analyzed to predict the optimal operation parameters of biomass feedstock. For pine woodchip gasification, the optimal particle size is 80 mesh or 0.17 mm, the preferred temperature is 850{$^\circ$}C, and the optimal S/B ratio is 1.4.},
  file = {/home/diego/Drive/Library/PhD/Gaseificação/Huang_Jin_2019_Investigation of biomass (pine wood) gasification.pdf}
}

