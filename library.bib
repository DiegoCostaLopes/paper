@article{zendehboudiDualApproachModelling2014,
  title = {A Dual Approach for Modelling and Optimisation of Industrial Urea Reactor: {{Smart}} Technique and Grey Box Model},
  shorttitle = {A Dual Approach for Modelling and Optimisation of Industrial Urea Reactor},
