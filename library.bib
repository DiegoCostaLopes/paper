@article{ayubBiomassSyngasModified2020,
  title = {Biomass to {{Syngas}}: {{Modified Non-Stoichiometric Thermodynamic Models}} for the {{Downdraft Biomass Gasification}}},
  shorttitle = {Biomass to {{Syngas}}},
  author = {Ayub, Hafiz Muhammad Uzair and Park, Sang Jin and Binns, Michael},
  year = {2020},
  month = jan,
  journal = {Energies},
  volume = {13},
  number = {21},
