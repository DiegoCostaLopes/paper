@article{imEconomicAssessmentOptimization2015,
  title = {Economic Assessment and Optimization of the {{Selexol}} Process with Novel Additives},
  author = {Im, Dasom and Roh, Kosan and Kim, Junhan and Eom, Yongseok and Lee, Jay H.},
  year = {2015},
  month = nov,
  journal = {International Journal of Greenhouse Gas Control},
  volume = {42},
  pages = {109--116},
  issn = {1750-5836},
  doi = {10.1016/j.ijggc.2015.08.001},
  urldate = {2024-01-05},
  abstract = {Despite the rising concern with climate change, carbon capture is yet to be implemented widely in industry due to its high cost. In this study, three candidate compounds, dimethyl carbonate (DMC), diethyl carbonate (DEC), and triacetin (TAT), are considered as additives for the solvent, Dimethyl Ethers of Polyethylene Glycol (DEPG), to improve the economics of the Selexol process. To discuss their economic feasibility, the 1-stage Selexol process employing each solvent additive is developed, and their operating and equipment costs are compared with those of the reference process (without the additive). The process employing the TAT additive is shown to be competitive with the reference process in terms of both operating and equipment costs, but DMC and DEC showed a serious problem in terms of solvent loss. For reducing the operating cost and CO2 capture cost further, the lean solvent temperature of the Selexol process with the TAT solvent additive is optimized at various CO2 capture rates.},
  keywords = {Economic evaluation,Optimization,Physical absorption,Selexol process,Solvent additive},
  file = {/home/diego/Software/Zotero/data/storage/EEGF52QJ/S1750583615300463.html}
}

