    \entry{theozzoMILPFrameworkOptimal2021}{article}{}{}
      \name{author}{2}{}{%
        {{hash=ff20ffb4c4709b1a01289c4ccec45207}{%
           family={Theozzo},
           familyi={T\bibinitperiod},
