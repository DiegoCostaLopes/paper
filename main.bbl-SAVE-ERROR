      \strng{fullhashraw}{09927a30cec7f8f549b77ca66478bacd}
      \strng{bibnamehash}{c094bd29c7d63e4645935cec1a484579}
      \strng{authorbibnamehash}{c094bd29c7d63e4645935cec1a484579}
      \strng{authornamehash}{c094bd29c7d63e4645935cec1a484579}
