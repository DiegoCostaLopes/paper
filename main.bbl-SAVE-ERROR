    \entry{jorapurSugarcaneLeafbagasseGasifiers1997}{article}{}{}
      \name{author}{2}{}{%
        {{hash=a6ba654356e0ac2310820b6d58adc0a8}{%
           family={Jorapur},
           familyi={J\bibinitperiod},
           given={Rajeev},
